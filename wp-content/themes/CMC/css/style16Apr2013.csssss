.arrow_wraper{clear:both;}
.left_arrow,.bx-next{background:url(../images/left_arrow.png) no-repeat; width:19px; height:28px; display:block; float:left; margin:72px 0 0 0;text-indent:-11111px !important;}
.left_arrow:hover,.bx-next:hover{background-position:left bottom;text-indent:-11111px !important;}
.right_arrow,.bx-prev{background:url(../images/right_arrow.png) no-repeat; width:19px; height:28px; display:block;text-indent:-11111px !important; float:right;margin:72px 0 0 0;}
.right_arrow:hover,.bx-prev:hover{background-position:right bottom; text-indent:-11111px !important;  }
.heading_box2{/*background:url(../images/heading_box2.png) no-repeat; width:307px; height:168px;*/ float:left;}
.heading_box3{/*background:url(../images/heading_box3.png) no-repeat; width:307px; height:168px;*/  float:left;}
