.floatleft{float:left;}
.floatright{float:right;}
.clear{clear:both;}
body{  background:#20181c; font-family:Arial, Helvetica, sans-serif;}
/*Header Css Start*/
#header {background: url(../images/header_bg.jpg) repeat-x; height:97px; padding: 0; min-width: 990px; width: 100%; padding:4px 0 0 0; /*border-bottom:1px solid #8c6a34;*/}
.header_inner{width:990px; margin:0 auto;}
a.logo{background:url(../images/logo.png) no-repeat; width:204px; height:90px; display:block; float:left; text-indent:-9999px;}
.header_right{float:right; /*width:727px;*/ padding:6px 0 0 0;}
.top_links{float:left; padding:11px 0 0 0;}
.top_links li{float:left; padding:0 0 0 17px; font-family: 'dinregular'; font-size:12px;}
.top_links li a{text-decoration:none; color:#aeaeae; text-transform:uppercase;}
.top_links li a:hover{color:#fff;}
.social_icon{float:left;padding:0 0 0 18px; border-left:1px solid #666; margin:10px 18px 0;}
.social_icon a.fb{background:url(../images/social_icon.png) no-repeat; width:8px; height:15px; display:block; float:left;}
.social_icon a.fb:hover{background-position:left bottom;}
.social_icon a.twitter{background:url(../images/social_icon.png) no-repeat -15px top; width:20px; height:15px; display:block; float:left; margin:0 0 0 7px;}
.social_icon a.twitter:hover{background-position:-15px bottom;}
.social_icon a.linkedin{background:url(../images/social_icon.png) no-repeat -43px top; width:14px; height:15px; display:block; float:left; margin:0 0 0 7px;}
.social_icon a.linkedin:hover{background-position:-43px bottom;}
.register_btn{ background: url("../images/register_btn.png") no-repeat scroll 0 0 transparent; display: block;float: left;height: 32px;width: 104px;}
 
.register_btn:hover{background-position:left bottom;}

.result_btn{ background: url("../images/result_btn.png") no-repeat scroll 0 0 transparent; display: block;float: left;height: 32px;width: 104px;}
 
.result_btn:hover{background-position:left bottom;}


.main_navigation{clear:both; padding:23px 0 0 0;padding:13px 0 0 0;}
.main_navigation ul{float:right;}
.main_navigation ul li{padding-left:34px; float:left;}
.main_navigation ul li a{color:#fff; font-size:14px; font-weight:bold; text-decoration:none; text-transform:uppercase;}
.main_navigation ul li a:hover{color:#FCA800;}
/*Header Css End*/

/*Banner Part Css Start*/
.wraper{/* height:705px;text-align: center;*/ clear:both; overflow: hidden; padding-bottom:50px;}
.wraper li{position:relative;}
.wraper li a img{ margin:0 0 0 0; /*display:block; max-width:100%;*/}
.wraper li .static_banner{ width:100%; text-align:left; clear:both; margin:auto; position:relative; width:990px; height:706px;}
.inner_image{ position:absolute; left:-550px; width:1920px;    }
.slide_content{position:absolute; /*right:-75px; bottom: 145px;*/right:-540px; bottom: 73px; width:330px;}
.slide_content h1{font-size:72px; color:#fff; text-transform:uppercase; text-align:left; font-family: 'capture_itregular'; font-weight:normal;line-height: 72px; text-shadow: 2px 0 0 #000000;} 
.slide_content p{font-size:14px; color:#fff; text-align:left;font-family: 'latoregular'; line-height:19px;} 
.doted_wraper{width:120px; margin:-72px auto 0; clear:both;}
.doted_wraper li{float:left; padding:0 0 0 5px;}
.doted_wraper li a{ width:25px; height:24px; display:block; background:url(../images/white_star.png) no-repeat;}
.doted_wraper li.active a{ width:25px; height:24px; display:block; background:url(../images/star.png) no-repeat;}
.doted_wraper li a:hover{background:url(../images/star.png) no-repeat;}
.banner_content{margin:0 auto; width:990px; position:relative; clear:both;}
.banner_nav{position:absolute; width:133px; height:191px; background:url(../images/nav_box.png) no-repeat; left:0; top:55px; z-index:99999; padding:111px 9px 0 9px;}
.banner_nav li{text-transform:uppercase; font-size:14px; color:#fff; text-decoration:none; cursor:pointer; background:url(../images/nav_bdr.png) repeat-x bottom; padding:5px 2px 7px 2px; text-align:left;font-family: 'open_sans_condensedbold';}
.banner_nav li:hover,.banner_nav li.active{color:#FCA800 !important;}
.banner_nav li:last-child{background:none;}
.banner_nav li.active a,.banner_nav li a:hover{color:#fca800;}
.banner_nav a{text-transform:uppercase; color:#fff; text-decoration:none;}

.event_main_block{margin:0 auto; padding:48px 0 0 0; clear:both; width:990px;font-family: 'latoregular';}
