#comments input[type=text],#comments input[type=password],#comments textarea{border-radius:5px; -moz-border-radius:5px;  -webkit-border-radius:5px; width:250px; padding:7px 10px; border:1px solid #ccc; float:left;}
#comments textarea{width:400px}
#comments input[type=text]:focus,#comments input[type=password]:focus,#comments textarea:focus{border:1px solid #333;}
.comment_list .share_div{  float: left;width: 268px;}
.comment_list .album_share{ top: 10px;}
.comment_list .album_share:before,.comment_list .album_share:after{  left: 10%;}
ol.commentlist{float:left;}
ol.commentlist li{padding:0 0 10px 0; float:left; border:1px solid #ccc; padding:10px; margin-bottom:10px; width:96%;}
ol.commentlist li img{margin:0 10px 0 0;}
.comment_content{width:85%;float:left; font-size:13px;}
ol.commentlist li ul{float:left; margin:10px 0 0 15px}
#nav-single{border-bottom:1px dashed #ccc;border-top:1px dashed #ccc; padding:2px 0 10px 0;}
 iframe[src^="//assets.pinterest"] {
 display: none!important;
}
#reply-title, #comments-title, #comments-title span{color:#494949; font-family:'latoregular'; font-size:16px; text-decoration:underline; }
#searchform label{font-size:16px; text-transform:uppercase;font-family: 'din_blackregular'; padding:0 0 17px 0;}
 #searchform input[type=text]{  border: 1px solid #CCCCCC;
    border-radius: 5px 5px 5px 5px;
    padding: 6px 10px; width: 375px;}
#searchform input[type=submit]{border-radius:5px;-moz-border-radius:5px;-webkit-border-radius:5px; padding:6px 10px; background:#FFAA00; border:none; cursor:pointer; font-size:14px; font-family:'din_blackregular'; color:#fff;}
.ticketsocket_div input[type=submit]{border-radius:5px;-moz-border-radius:5px;-webkit-border-radius:5px; padding:6px 10px; background:#FFAA00; border:none; cursor:pointer; font-size:14px; font-family:'din_blackregular'; color:#fff;float:left;}
#nav-below .assistive-text{ display: none; }
#nav-below a,.ticketsocket_div a{ font-family: 'latoregular'; font-size: 13px; color: #494949; text-decoration: none; }
#nav-below a:hover, .ticketsocket_div a:hover{ text-decoration: underline;}
